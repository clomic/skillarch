gen_pass() { grep -xP "[a-z]{4,6}" /opt/lists/SecLists/Miscellaneous/lang-english.txt|shuf -n6|tr '\n' '-'|sed 's/-$//'; }
crypt-enc() (set -e;
    PASS=$(gen_pass)
    tar zcf "$1.tar.gz" "$1";
    openssl enc -aes-256-cbc -pbkdf2 -in "$1.tar.gz" -out "$1.tar.gz.enc" -pass pass:"${PASS}"
    rm "$1.tar.gz"; printf "Password: %s" "${PASS}" | cpyc;
    echo -e "\nEncrypted file: $1.tar.gz.enc")
crypt-dec() (set -e;openssl enc -d -aes-256-cbc -pbkdf2 -in "$1" -out "${1%.enc}";tar xf "${1%.enc}";rm "${1%.enc}")

# Python
pv() { if [ -z "$1" ] ; then echo "Use: pv 3.12" ; else mise use "python@$1" ; mise config set env._.python.venv.path .venv ; mise config set --type list env._.python.venv.uv_create_args -- --seed ; mise config set --type bool env._.python.venv.create true ; mise exec -- which python ; mise exec -- python --version ; fi; }
